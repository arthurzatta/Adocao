{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals" : {
      "Atomics" :"readonly",
      "SharedArrayBuffer" : "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "import/extensions" :[
        "error",
        "ignorePackages",
        {
          "js" : "never"
        }
      ]
    }
}
